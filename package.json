{
  "name": "ks-killer",
  "description": "Kill your Switches with ease",
  "author": "Sixian Li",
  "version": "0.1.0",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "bin": {
    "ks-killer": "./bin/index.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/index.ts",
      "!src/**/cli.ts"
    ]
  },
  "scripts": {
    "start": "yarn build && node ./lib/cli.js --help",
    "dev": "tsc --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "babel-jest": "^27.2.5",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "ts-morph": "^12.0.0",
    "yargs": "^17.2.1"
  }
}
